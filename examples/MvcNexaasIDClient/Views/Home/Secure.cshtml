@{
    ViewData["Title"] = "Secure";
}
<h1>Secure</h1>

<div class="row">
    <div class="col-md-8">
        <div>


            <table class="table table-hover">
                <thead>
                <tr>
                    <th colspan="2" class="text-center">
                        <h2>Claims</h2>
                    </th>
                </tr>
                <tr>
                    <th>Type</th>
                    <th>Value</th>
                </tr>
                </thead>
                <tbody>
                @foreach (var claim in User.Claims)
                {
                    <tr>
                        <td>@claim.Type</td>
                        <td>@claim.Value</td>
                    </tr>
                }
                </tbody>
            </table>
        </div>
    </div>
</div>